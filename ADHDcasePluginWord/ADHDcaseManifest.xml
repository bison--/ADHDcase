<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
    <Id>ADHDcase</Id>
    <Version>0.0.1</Version>
    <ProviderName>bison</ProviderName>
    <DefaultLocale>en-US</DefaultLocale>
    <DisplayName DefaultValue="ADHDcase" />
    <Description DefaultValue="This add-in makes the first syllable of each word in the selected text bold."/>
    <Hosts>
        <Host Name="Document" />
    </Hosts>
    <DefaultSettings>
        <SourceLocation DefaultValue="~remoteAppUrl/ADHDcase.html" />
    </DefaultSettings>
    <Permissions>ReadWriteDocument</Permissions>
    <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
        <Hosts>
            <Host xsi:type="Document">
                <DesktopFormFactor>
                    <GetStarted>
                        <Title resid="GetStarted.Title"/>
                        <Description resid="GetStarted.Description"/>
                        <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
                    </GetStarted>
                    <FunctionFile resid="Commands.Url" />
                    <ExtensionPoint xsi:type="Events">
                        <Event id="DocumentSelectionChanged" handler="ADHDcase.onSelectionChange"/>
                    </ExtensionPoint>
                </DesktopFormFactor>
            </Host>
        </Hosts>
        <Resources>
            <bt:Images>
                <bt:Image id="Icon.16x16" DefaultValue="~remoteAppUrl/Images/Icon.16x16.png" />
                <bt:Image id="Icon.32x32" DefaultValue="~remoteAppUrl/Images/Icon.32x32.png" />
                <bt:Image id="Icon.80x80" DefaultValue="~remoteAppUrl/Images/Icon.80x80.png" />
            </bt:Images>
            <bt:Urls>
                <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://github.com/bison--/ADHDcase" />
                <bt:Url id="Commands.Url" DefaultValue="~remoteAppUrl/ADHDcase.html" />
            </bt:Urls>
            <bt:ShortStrings>
                <bt:String id="GetStarted.Title" DefaultValue="ADHDcase" />
                <bt:String id="Commands.Group.Label" DefaultValue="ADHDcase" />
            </bt:ShortStrings>
            <bt:LongStrings>
                <bt:String id="GetStarted.Description" DefaultValue="This add-in makes the first syllable of each word in the selected text bold."/>
            </bt:LongStrings>
        </Resources>
    </VersionOverrides>
</OfficeApp>
